/*==================== resets*/
/* 
html5doctor.com Reset Stylesheet
v1.6.1
Last Updated: 2010-09-17
Author: Richard Clark - http://richclarkdesign.com 
Twitter: @rich_clark
*/
html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code,
del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var,
b, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video{
    margin:0;
    padding:0;
    border:0;
    outline:0;
    font-size:100%;
    vertical-align:baseline;
    background:transparent;
}
body{
    line-height:1;
}
article,aside,details,figcaption,figure,
footer,header,hgroup,menu,nav,section{ 
	display:block;
}
nav ul{
    list-style:none;
}
blockquote, q{
    quotes:none;
}
blockquote:before, blockquote:after,
q:before, q:after{
    content:'';
    content:none;
}
a{
    margin:0;
    padding:0;
    font-size:100%;
    vertical-align:baseline;
    background:transparent;
}
/* change colours to suit your needs */
ins{
    text-decoration:none;
}
/* change colours to suit your needs */
mark{
    font-style:italic;
    font-weight:bold;
}
del{
    text-decoration: line-through;
}
abbr[title], dfn[title]{
    cursor:help;
}
table{
    border-collapse:collapse;
    border-spacing:0;
}
hr {
    display:block;
    height:1px;
    border:0;   
    margin:1em 0;
    padding:0;
}
input, select{
    vertical-align:middle;
}
ul, ul li{
	list-style:none;
}
/* END resets ================*/

/*================ clearfix */
.clearfix:after {
	visibility:hidden;
	display:block;
	font-size:0;
	content:" ";
	clear:both;
	height:0;
	}
* html .clearfix             { _zoom: 1; } /* IE6 */
*:first-child+html .clearfix { zoom: 1; } /* IE7 */
/* END clearfix ============= */

.clear, .space, .xl-space, .xxl-space{
	clear: both;
	display: block;
	overflow: hidden;
	visibility: hidden;
	width: 0;
	height: 0;
}
.space{
	height: 10px;
}
.xl-space{
	height:20px;
}
.xxl-space{
	height:45px;
}
.center{
	text-align:center;
}
a{
	text-decoration:underline;
	color:#0bc091;
}
a:hover{
	color:#000;
}
.float-left{
	float:left;
}
.float-right{
	float:right;
}
a.bt-style1, a.bt-style2{	
	color:#fff;
	display:block;
	float:left;
	text-transform:uppercase;	
	font-weight:bold;
	text-align:center;	
	text-decoration:none;
}
a.bt-style1{
	background:#00c491;
	font-size:16px;
	padding:7px 20px;
}
a.bt-style2{
	background:#b93b51;
	font-size:14px;
	padding:7px 15px;
}
a.bt-style1:hover, a.bt-style2:hover{
	background:#000;
}
.sep1{
	float:left;
	background:#ddd;
	width:7px;
	font-size:0;
	height:215px;
}
.sep2{
	float:left;
	padding-right:10px;
}
.padding-l18{
	padding-left:18px;
}
/* ======== END GENERAL STUFF */
body{
	font:normal 13px Arial;
	color:#222;
}
header{
	padding:22px 0 35px;
}
.inner{
	width:960px;
	margin:auto;
}
header .logo img{
	vertical-align:top;
}
header .logo .float-right{
	padding:22px 0 0;
	font-size:19px;
	color:#000;
	white-space:nowrap;
}
header nav{
	background:#000;
	padding:17px 0 0;
	margin:22px 0 0;
}
header nav ul li{
	float:left;
	position:relative;
	height:59px;
}
header nav a{
	display:block;
	width:125px;
	height:59px;
	padding:0 0 0 10px;
	text-decoration:none;
	color:#ddd;
	font-size:15px;
	background:url(../images/menu-bg.gif) no-repeat left top;
}
header nav a span{
	display:block;
	font-size:11px;
	color:#809690;
	cursor:pointer;
}
header nav a.selected, nav a:hover, header nav ul li:hover a{
	color:#37e5b8;
	background-position:0 -100px;
}
header nav a.selected span, nav a:hover span{
	color:#76a397;
}
header nav ul li ul{
	display:none;
	position:absolute;
	background:url(../images/slider-nav-bg.png) repeat;
	z-index:1000;
	width:180px;
	padding:10px 5px;	
}
header nav ul li ul li{
	float:none;
	display:block;
	padding:0 0 10px;
	height:auto;
}
header nav ul li ul li a, header nav ul li:hover ul li a{
	background:none;
	height:auto;
	width:auto;
	font-size:12px;
	padding:5px 7px;
	color:#fff;
	background:none;
	font-weight:normal;
}
header nav ul li ul li a:hover, header nav ul li:hover ul li a:hover{
	background-color:#06211A;
	color:#37E5B8;
}
header nav ul li:hover ul{
	display:block;
}
#container{
	margin-bottom:35px;
	position:relative;
}
.sub-page{
	padding-top:15px;
}
.breadcrumb{
	position:absolute;
	left:0;
	top:-20px;
}
.breadcrumb ul li{
	display:inline;
}
.breadcrumb a{
	text-decoration:none;
}
.breadcrumb li.current a{
