	font-weight:bold;
	color:#000;
	line-height:39px;
	margin:0 0 20px;
}
h2{
	font-size:20px;
	font-weight:normal;
	color:#000;
	margin:0 0 12px;
}
.sub-page h1{
	font-size:27px;
	font-weight:normal;
	line-height:normal;
}
.sub-page h2{
	font-size:20px;
	border-bottom:1px solid #000;
	padding:0 0 3px;
	line-height:normal;
	margin:15px 0 10px;
}
.sub-page h3{
	font-size:17px;
	font-weight:bold;
	color:#000;
	margin:7px 0;
}
.welcome{
