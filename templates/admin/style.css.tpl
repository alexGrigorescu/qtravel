{literal}
html { height: 100%;}
body {background-color: #ffffff; margin: 0; padding: 0; color: #000000; font-size: 10px; font-family: verdana; height: 100%;}
a, a:link { color: #000000; text-decoration:none; font-size: 12px;}
a:visited, a:hover, a:active { color: #000000 }
img {border-color:#333333;}
td { font-size: 11px; text-decoration: none; font-family: verdana; line-height:13px; }
input.text, textarea {font-size: 11px; border: 1px solid #CFCFCF; background-color:#ffffff; color:#000000; padding:1px;}

div.nav {padding: 6px 0px 5px 0px; text-align:center;}
div.nav a {list-style-type: none; font-size: 12px; font-weight:normal;line-height:20px;}

div.form {width:100%;}
div.form td.label { float:left; font-size:12px; text-align:left; color:#464646; font-weight:bold; width:120px; }
div.form td.element { float:left; text-align:left; width:300px;}
div.form td.element input.text, input.file, textarea, 4 {border: 1px solid #CFCFCF; background-color: #ffffff; width: 300px; padding:2px; margin:0px; vertical-align: middle; }
div.form td.element input.choose{border: 1px solid #CFCFCF; background-color: #DEE7ED; width: 20px; height:20px; margin:0px; display:inline;}
div.form div.header, div.form h2{ background: url({/literal}{$img_path}{literal}header_bg.gif) 50% 50% #E9E8F2; color: #000; font-weight:bold; font-size:12px; padding: 3px; padding-left: 5px; height:16px; margin-bottom:0px;}
div.form div.footer { float:left; padding:5px;	}
div.form div.footer input {background-color:#f6f6f6; color:#000;margin-right:1px;}
div.form div.help { float:left;  width:30%; font-size:10px; text-align:left;}
div.form div.help { }
div.form select { }
div.form textarea { border: 1px solid #CFCFCF; background-color:#ffffff; width: 300px; height:50px;}
div.form textarea.content { width: 300px; height:100px;}
div.form div.div_wymeditor { width: 730px; height:320px;}

div.search {width:100%;}
div.search td.label { font-size:12px; text-align:left; color:#464646; font-weight:bold; width:120px; }
div.search td.element { text-align:left; width:150px;}
div.search td.element input.text, input.file, textarea{border: 1px solid #CFCFCF; background-color: #ffffff; width: 200px; padding:2px; margin:0px; vertical-align: middle; }
div.search td.element input.choose{border: 1px solid #CFCFCF; background-color: #DEE7ED; width: 20px; height:20px; margin:0px; display:inline;}
div.search div.header, div.search h2{ background: url({/literal}{$img_path}{literal}header_bg.gif) 50% 50% #E9E8F2; color: #000; font-weight:bold; font-size:12px; padding: 3px; padding-left: 5px; height:16px; margin-bottom:0px;}
div.search div.footer { 	}
div.search div.footer input {background-color:#f6f6f6; color:#000;margin-right:1px;}
div.search div.help { width:30%; font-size:10px; text-align:left;}
div.search div.help { }

table.grid { font-size:10px; width:100%;  }
table.grid td.title a { font-weight:bold; color: #ff0000; }
table.grid td.header { background: url({/literal}{$img_path}{literal}header_bg.gif) 50% 50% #E9E8F2; color: #000; font-weight:bold; font-size:13px; padding: 3px; padding-left: 5px; height:16px; margin-bottom:0px;}
table.grid td.row0 { }
table.grid td.row1 { }
table.grid td.cell0 { padding:5px; font-size:12px; background-color: #f7f7f7; height:20px; color: #000000;}
table.grid td.cell1 { padding:5px; font-size:12px; background-color: #fefefe; height:20px; color: #000000; }
table.grid td.norec { background-color: #000000; }
table.grid td.footer { font-size:13px; background-color: #fefefe; height:25px}
table.grid td.cell0 A { color: #0096FF; }
table.grid td.cell1 A { color: #0096FF; }
table.grid td.header_form input.text {padding:3px; padding-bottom:2px; width:100%}
table.grid td.header_form input.button {border: 1px solid #CFCFCF; background-color: #DEE7ED;}

table.news {width:100%;}
table.news td.date {}
table.news td.title {}
table.news td.leadtext {}
table.news td.content {}

.code {width:204px; height:40px;padding-top:5px;}
div.error {color: #464646; text-align:center; width: 95%; height:60px; background-color: #fefefe; padding:15px; margin:auto; border:1px solid #E9E8F2;}
div.error h2{font-size:15px;}
div.error b{font-size:12px; color:#ff0000;}

.dtree { color: #666; white-space: nowrap;}
.dtree img { border: 0px; vertical-align: middle;}
.dtree a { color: #333; text-decoration: none;}
.dtree a.node, .dtree a.nodeSel { white-space: nowrap; padding: 1px 2px 1px 2px;}
.dtree a.node:hover, .dtree a.nodeSel:hover { color: #333; text-decoration: underline;}
.dtree a.nodeSel { background-color: #c0d2ec;}
.dtree .clip { overflow: hidden;}

.win_head {background: url({/literal}{$img_path}{literal}header_bg.gif) 50% 50%; color: #000; font-wight:bold;	border: 0px; vertical-align:middle; height:22px; cursor: pointer; margin-left: 0px; width:100%; cursor: move;}
.win_close { width:18px; height:18px; float:right; text-align:center; vertical-align:middle; font-weight: bold;	font-size:14px;	cursor: pointer; margin-right: 2px; margin-top: 2px; background: url({/literal}{$img_path}{literal}greenclose.png);background-position: 10% 90%; background-repeat: no-repeat;}
.win_body {	margin-top: 5px;padding: 0px; overflow: scroll; height:270px;}
.win_title{font-weight:bold; padding:3px;}
.win_area {	margin:0px;	padding:0px; display:none; position:absolute; top:0px;	left:0px; border: 1px solid #CFCFCF; background-color: #f9f9f1;	vertical-align: top; 	}

.clearer {clear: both;}
{/literal}